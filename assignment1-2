#5.
l=[10,20,30,40,50]
a=len(l)
for i in range(a-1,-1,-1):
  print(l[i],end=" ")

#6.
a=-10
for i in range(0,10):
  print(a+i,end=" ")

#7.
for i in range(2,100):
  count=0
  for j in range(3,i):
    if(i%j==0):
      count=count+1
      break
    else:
      continue
  if(count==0):
    print(i,end=" ")

#8.
n=int(input("enter the number:"))
a=0
b=1
print(a,b,end=" ")
while (n>0):
  c=a+b
  a=b
  b=c
  print(c,end=" ")
  n=n-1

#9.
n=int(input("enter the number:"))
a=1
while(n>0):
  a=a*n
  n=n-1
print(a)

#10.
a=input("enter the number:")
b=list(a)
for i in range(len(b)-1,-1,-1):
  print(b[i],end="")
